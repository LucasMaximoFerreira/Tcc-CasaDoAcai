<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundomenu"
    android:id="@+id/drawer_layout"
    tools:context=".ComprasProduto.ComprarAcai2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/include4"
            layout="@layout/main_toolbar" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="284dp"
            android:layout_height="259dp"
            android:background="@drawable/acaifundo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/include4"
            app:layout_constraintVertical_bias="0.023" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/chicken_pie"
            android:text="ADICIONAIS"
            android:textColor="#fff"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.254"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            app:layout_constraintVertical_bias="0.035" />

        <TableLayout
            android:id="@+id/tableLayout"
            android:layout_width="149dp"
            android:layout_height="237dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.194"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            app:layout_constraintVertical_bias="0.444">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkAmendoim"
                    android:layout_width="135dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Amendoim"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"
                    android:textColor="#fff" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkGranulado"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Granulado"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"

                    android:textColor="#fff" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkGranola"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Granola"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"
                    android:textColor="#fff" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkLeiteEmPo"
                    android:layout_width="127dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Leite em Pó"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"
                    android:textColor="#fff" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkLeiteCondensado"
                    android:layout_width="147dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Leite Condensado"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"
                    android:textColor="#fff" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkChocobol"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Chocobol"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"
                    android:textColor="#fff" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkPacoca"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Paçoca"
                    android:buttonTint="@color/dd"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9"
                    android:textColor="#fff" />
            </TableRow>
        </TableLayout>

        <Button
            android:id="@+id/btnComprarAcai2"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:background="@drawable/fundo_botao_logar"
            android:fontFamily="@font/chicken_pie"
            android:paddingBottom="4dp"
            android:text="CONTINUAR"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976" />

        <TableLayout
            android:id="@+id/tableLayout3"
            android:layout_width="172dp"
            android:layout_height="223dp"
            android:layout_marginTop="52dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.933"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            app:layout_constraintVertical_bias="0.045">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkMorango"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/dd"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Morango"
                    android:textColor="#fff"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkChocolate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/dd"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Chocolate"
                    android:textColor="#fff"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkCaramelo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/dd"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Caramelo"
                    android:textColor="#fff"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkMenta"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/dd"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Menta"
                    android:textColor="#fff"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkTutiFruti"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/dd"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Tuti-Fruti"
                    android:textColor="#fff"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:id="@+id/chkMaracuja"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/dd"
                    android:fontFamily="@font/chicken_pie"
                    android:text="Maracujá"
                    android:textColor="#fff"
                    android:textColorHighlight="#9300E9"
                    android:textColorLink="#9300E9" />
            </TableRow>

        </TableLayout>

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/chicken_pie"
            android:text="CALDAS"
            android:textColor="#fff"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/tableLayout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.411"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            app:layout_constraintVertical_bias="0.4" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@android:color/white">

        <include
            layout="@layout/activity_main_nav_drawer"/>
    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>